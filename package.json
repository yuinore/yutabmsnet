{
  "name": "yutabmsnet",
  "version": "1.0.0",
  "description": "My portfolio site.",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --config tools/webpack/webpack.config.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config tools/webpack/webpack.config.prod.js",
    "deploy": "yarn build && yarn gh-pages -d dist/",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src/",
    "postbuild": "react-snap ; cp -R assets/ dist/"
  },
  "reactSnap": {
    "puppeteerArgs": [
      "--no-sandbox"
    ],
    "include": [
      "/",
      "/secret"
    ],
    "source": "./dist"
  },
  "devDependencies": {
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.8",
    "@types/webpack-env": "^1.18.8",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "cross-env": "^10.0.0",
    "css-loader": "^7.1.2",
    "eslint": "^9.34.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^9.1.0",
    "gh-pages": "^6.3.0",
    "html-webpack-plugin": "^5.6.4",
    "less": "^4.4.1",
    "less-loader": "12.3.0",
    "mini-css-extract-plugin": "^2.9.4",
    "react-snap": "^1.23.0",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.2",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "dependencies": {
    "@types/react-router-dom": "^5.3.3",
    "@types/react-transition-group": "^4.4.12",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^7.8.2",
    "react-share": "^5.2.2",
    "react-transition-group": "^4.4.2"
  }
}
